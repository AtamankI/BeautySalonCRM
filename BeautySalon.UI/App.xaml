<Application x:Class="BeautySalon.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BeautySalon.UI"
             xmlns:vml="clr-namespace:BeautySalon.UI.Infrastructure"
             StartupUri="Views/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary >
            <vml:ViewModelLocator x:Key="viewModelLocator"></vml:ViewModelLocator>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/Style.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
